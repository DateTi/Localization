{
    "name": "dateti/localization",
    "description": "Localization for DateTi",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Jan Galek",
            "email": "admin@gcore.cz"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "DateTi\\": "src"
        }
    },
    "scripts": {
        "cs": "tests/coding-standard/vendor/bin/ecs check src --config tests/coding-standard/coding-standard-php71.yml",
        "csf": "tests/coding-standard/vendor/bin/ecs check src --config tests/coding-standard/coding-standard-php71.yml --fix"
    }
}
